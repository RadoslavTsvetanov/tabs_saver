{
  "manifest_version": 3,
  "name": "My Background Extension",
  "version": "1.0",
  "permissions": ["tabs","scripting","storage"],
  "background": {
    "scripts": ["background.js"],
    "persistent": true
  },
  "action": {
    "default_popup":"index.html"
  },
  "host_permissions": ["http://*/*","https://*/*"]
  
}
